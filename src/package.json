    "tmi.js": "^1.8.5",
    "typescript": "^4.5.3"
  },
  "devDependencies": {
    "@types/node": "^16.10.2",
    "@types/sqlite3": "^3.1.7",
    "@types/tmi.js": "^1.7.2"
  }
}
