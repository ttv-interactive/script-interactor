{
  "name": "script-interactor",
  "version": "1.0.1",
  "author": "viktorholk",
  "description": "Twitch chatbot for executing scripts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/viktorholk/script-interactor.git"
  },
  "bugs": {
